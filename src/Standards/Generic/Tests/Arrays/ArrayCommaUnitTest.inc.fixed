<?php
$var = [1, 2, 3];
$var = [
    1,
    2,
    3,
];

$var = [
    1 => 'one',
    2 => 'two',
    3 => 'three',
];

$var = array(
    'one' => function() {
        $foo = [1, 2, 3];
        $bar = [
            1,
            2,
            3,
        ];
    },
    'two' => 2,
);

return [
    [
        'foo' => true,
    ],
];

